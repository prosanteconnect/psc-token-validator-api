openapi: 3.0.0
info:
  version: '1.0'
  title: psc-token-validator @project.version@
  description: This API calls PSC to check token validity
  contact:
    email: esignsante@esante.gouv.fr
servers:
  - url: 'https://psc-token-validator.psc.esante.gouv.fr/v1'
    description: Sandbox esignsante.psc
paths:
  '/tokenvalidator/{token}':
    get:
      summary: Vérifie la validité d'un token prosanteconnect'
      description: 'Opération qui permet au client de lister les opérations offertes par le composant eSignSante-PSC.  '
      tags:
        - tokenvalidator
      responses:
        '200':
          description: Requête en succès.
          content:
            application/json:
              schema:
                type: string            
        '503':
          description: Prosante Connect inaccessible.
        '500':
          description: Erreur interne du serveur.
      operationId: token-validator
      parameters:
        - schema:
            type: string
          name: token
          in: path
          required: true
          description: token à valider
